{
  "manifest_version": 3,
  "name": "複数URL一括オープン",
  "version": "1.0.0",
  "description": "複数のURLを一度に貼り付けて、すべてのURLを新しいタブで開くことができます。",
  "permissions": [
    "tabs"
  ],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icons/icon16.png",
      "48": "icons/icon48.png",
      "128": "icons/icon128.png"
    }
  },
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  }
}

